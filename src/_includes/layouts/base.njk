{% set seoSiteName = 'Greythink Labs' %}
{% set seoTitle = (title + ' - ' + seoSiteName) %}



<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        {{ seoTitle }}
    </title>

    {# Fonts #}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans&family=Mali:wght@600;700&family=Playfair+Display:wght@700&display=swap" rel="stylesheet">

    {# CSS #}
    <link rel="stylesheet" href="{{ 'build/style.css'|version }}">
  </head>
  
  <body>
    <main id="main-content" tabindex="-1">
        {{- content|safe -}}
    </main>
  </body>
</html>